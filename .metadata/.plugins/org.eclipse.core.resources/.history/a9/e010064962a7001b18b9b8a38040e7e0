public class SampleTest_StepDef {
	
	initWeb initialize = new initWeb();
	
	@Given("I access the something")
	public void i_access_the_something() {
		initialize.webChrome("https://google.com");
	    	assertEquals( true, true);
	 
//	    throw new io.cucumber.java.PendingException();
	}

	@When("I input something")
	public void i_input_something() {
		 System.out.println("asdasd34324");
		 assertTrue("Truelalu", true);
		 
	}
	@Then("I see something")
	public void i_see_something() {
		 System.out.println("asdfdgfg        asd");

}
}
